  </sharedAnimations>


  <sharedBoardAnimations>
    <boardAnimation name="AnnieQ" center="pressedKey">
      <animation x="5" y="-1" name="AnnieQGradient" start="0"/>
      <animation x="6" y="-1" name="AnnieQGradient" start="0"/>

      <animation x="6" y="0" name="AnnieQGradient" start="2"/>
      <animation x="7" y="-1" name="AnnieQGradient" start="2"/>
      <animation x="5" y="-2" name="AnnieQGradient" start="2"/>

      <animation x="7" y="0" name="AnnieQGradient" start="4"/>
      <animation x="8" y="-1" name="AnnieQGradient" start="4"/>
      <animation x="6" y="-2" name="AnnieQGradient" start="4"/>

      <animation x="8" y="0" name="AnnieQGradient" start="6"/>
      <animation x="9" y="-1" name="AnnieQGradient" start="6"/>
      <animation x="7" y="-2" name="AnnieQGradient" start="6"/>

      <animation x="9" y="0" name="AnnieQGradient" start="8"/>
      <animation x="10" y="-1" name="AnnieQGradient" start="8"/>
      <animation x="8" y="-2" name="AnnieQGradient" start="8"/>

      <animation x="10" y="0" name="AnnieQGradient" start="10"/>
      <animation x="11" y="-1" name="AnnieQGradient" start="10" />
      <animation x="9" y="-2" name="AnnieQGradient" start="10"/>
    </boardAnimation>

    <!--
    <boardAnimation center="key" keyname="F">
    
    </boardAnimation>
    -->
  </sharedBoardAnimations>


  <KeyGroups>
    <group name="InventoryActives">
      <key name="1" />
      <key name="2" />
      <key name="3" />
      <key name="5" />
      <key name="6" />
      <key name="7" />
    </group>
  </KeyGroups>

  <Map name="default">
    <all>
      <colorGroup type="default">
        <color r="0" g="0" b="0" />
      </colorGroup>
    </all>
    <Key keyname="Q">
      <colorGroup type="default">
        <animation name="Ability1" />
      </colorGroup>
      <colorGroup type="onRelease" boardAnimation="AnnieQ">
        <color r="0" g="0" b="0" />
      </colorGroup>
    </Key>
    <Key keyname="W">
      <colorGroup type="default">
        <animation name="Ability2" />
      </colorGroup>
    </Key>
    <Key keyname="E">
      <colorGroup type="default">
        <animation name="Ability3" />
      </colorGroup>
    </Key>
    <Key keyname="R">
      <colorGroup type="default">
        <animation name="Ability4" />
      </colorGroup>
    </Key>
    <Key keyname="D">
      <colorGroup type="default">
        <animation name="SkillHeal" />
      </colorGroup>
    </Key>
    <Key keyname="F">
      <colorGroup type="default">
        <animation name="SkillFlash" />
      </colorGroup>
    </Key>
    <Key keyname="B">
      <colorGroup type="default">
        <animation name="BackPort" />
      </colorGroup>
    </Key>
    <KeyGroup name="InventoryActives">
      <colorGroup type="default">
        <animation name="UseFullKey" />
      </colorGroup>
    </KeyGroup>
    <Key keyname="4">
      <colorGroup type="default">
        <animation name="Trinket" />
      </colorGroup>
    </Key>
    <Key keyname="G">
      <colorGroup type="default">
        <animation name="Alert" />
      </colorGroup>
    </Key>
    <Key keyname="V">
      <colorGroup type="default">
        <animation name="Retreat" />
      </colorGroup>
    </Key>

  </Map>
  
</Theme>